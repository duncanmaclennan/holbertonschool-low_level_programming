#include <stdio.h>
#include "main.h"
/**
 * _strspn - tests if character is digit
 * @s: the first integer
 * @accept: the bytes
 * Return: the conversion
 */
void print_chessboard(char (*a)[8]) {
    for (int i = 0; i < 8; i++) {
        for (int j = 0; j < 8; j++) {
            printf("%c ", a[i][j]);
        }
        printf("\n");
    }
}
